@model dynamic
@using Web.Principal.Utils
@using Web.Principal.HtmlHelpers
@using Microsoft.Extensions.Configuration
@inject IConfiguration Configuration

<!DOCTYPE html>
<html lang="en">
<head>

    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    <title> @ViewData["Title"] </title>
    <link rel="icon" type="image/x-icon" href="~/img/favicon.png" />


    <link href="https://cdn.jsdelivr.net/npm/simple-datatables@latest/dist/style.css" rel="stylesheet" />
    <link href="@Url.Content("~/css/styles.css")?version=1" rel="stylesheet" />
    <link href="@Url.Content("~/css/style_main.css?version=1")" rel="stylesheet" />
    <link rel="icon" type="image/x-icon" href="~/img/favicon.png" />
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <script  src="/js/all.min.js" ></script>
    
    <script src="/js/feather.min.js" ></script>

    <script type="text/javascript">
        var xa = 0;
        var btnNotificacion = false;
    </script>


</head>


@{
    var usuario = Context.Session.GetUserContent();
}

<body class="nav-fixed">
    <partial name="_BarraSuperior" />

    <div id="layoutSidenav">
        <partial name="_TopNavbar" />

        <div id="layoutSidenav_content">
            <main>
                @RenderBody()
            </main>

            <div id="layoutAuthentication_footer">
                <partial name="_Footer" />
            </div>

        </div>
    </div>


    <partial name="_EmpresaTransmares" />


    <link href="~/lib/jquery-ui/dist/css/themes/base/jquery-ui.min.css" rel="stylesheet" />
    <script src="~/lib/jquery/dist/jquery.js"></script>
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/js/bootstrap.bundle.min.js" crossorigin="anonymous"></script>
    <script src="~/lib/browser-detect-javascript/detect.min.js"></script>
    <script src="~/lib/popper.js/dist/umd/popper.js"></script>
    <script src="~/lib/jquery-ui/dist/js/jquery-ui.min.js"></script>
    <script src="~/lib/jquery-ui/dist/js/jquery.ui-contextmenu.min.js"></script>
    <script src="~/js/litepicker.bundle.js"></script>
    <script src="~/js/signalr/dist/browser/signalr.js"></script>
    <script src="~/js/scripts.js"></script>
    <script src="~/js/site.js?version=1"></script>



    <script>
        var userId ="@usuario.idUsuario.ToString()";
        var strLayouCodigoUsuario = "@Html.Encriptar(usuario.idUsuario.ToString())";
    </script>



    <script src="~/js/usernotification.js?version=203"></script>

    @RenderSection("Scripts", required: false)

    <script src="~/js/tmpdwac.general.js"></script>

    <partial name="_SesionScripts" />


</body>
</html>
